{
  "name": "react-project-template",
  "version": "1.0.0",
  "description": "A react project template",
  "main": "main.js",
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "start": "webpack-dev-server --hot --inline",
    "build:dev": "webpack -d --devtool source-map ./src/main.js ./dist/js/bundle.js",
    "dist": "WEBPACK_CONFIG='build' webpack -p"
  },
  "author": "John Doe MG",
  "license": "MIT",
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "css-loader": "^0.25.0",
    "deep-freeze-node": "^1.1.2",
    "enzyme": "^2.4.1",
    "expect": "^1.20.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "react-addons-test-utils": "^15.3.1",
    "redux-devtools": "^3.3.1",
    "redux-mock-store": "^1.2.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  }
}
